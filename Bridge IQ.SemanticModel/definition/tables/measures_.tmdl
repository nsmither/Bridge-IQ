	measure 'Total Points Season' = ```
			
			VAR TodayDate = TODAY()
			VAR SeasonStartDate = DATE(2025,10,19)
			VAR WeeklyTable =
			    FILTER(
			        ADDCOLUMNS(
			            SUMMARIZECOLUMNS(
			                'Employee Photos'[EmployeeName],
			                'ffl week schedule'[start of week],
			                'ffl week schedule'[end of week ]
			            ),
			            "@StartOfWeek", 'ffl week schedule'[start of week],
			            "@EndOfWeek", 'ffl week schedule'[end of week ]
			        ),
			        [@StartOfWeek] >= SeasonStartDate &&
			        [@StartOfWeek] <= TodayDate
			    )
			RETURN
			SUMX(
			    WeeklyTable,
			    VAR WeekStart = [@StartOfWeek]
			    VAR WeekEnd = [@EndOfWeek]
			    VAR Emp = [EmployeeName]
			    RETURN
			        CALCULATE(
			            [Total FFL Points],
			            KEEPFILTERS(
			                DATESBETWEEN(
			                    'Calendar'[Date],
			                    WeekStart,
			                    WeekEnd
			                )
			            ),
			            KEEPFILTERS(
			                TREATAS({ Emp }, 'Employee Photos'[EmployeeName])
			            )
			        )
			)
			
			
