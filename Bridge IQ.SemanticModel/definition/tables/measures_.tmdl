	measure 'Team Total Wins' = ```
			
			IF(
			    HASONEVALUE('ffl_lineup'[team]),
			    VAR CurrentTeam = SELECTEDVALUE('ffl_lineup'[team])
			    VAR TodaysDate = TODAY()
			    RETURN
			    SUMX (
			        FILTER (
			            'ffl week schedule',
			            'ffl week schedule'[end of week ] < TodaysDate &&
			            (
			                'ffl week schedule'[ team 1A] = CurrentTeam ||
			                'ffl week schedule'[team 1B] = CurrentTeam ||
			                'ffl week schedule'[ team 2A] = CurrentTeam ||
			                'ffl week schedule'[team 2B] = CurrentTeam
			            )
			        ),
			        VAR CurrentWeek = 'ffl week schedule'[Weeks ]
			        VAR Team1A = 'ffl week schedule'[ team 1A]
			        VAR Team1B = 'ffl week schedule'[team 1B]
			        VAR Team2A = 'ffl week schedule'[ team 2A]
			        VAR Team2B = 'ffl week schedule'[team 2B]
			
			        VAR Score1A = CALCULATE ( 
			            [FFL total points final v5], 
			            ALL('ffl_lineup'),
			            ALL('ffl week schedule'),
			            'ffl_lineup'[team] = Team1A,
			            'ffl week schedule'[Weeks ] = CurrentWeek
			        )
			        VAR Score1B = CALCULATE ( 
			            [FFL total points final v5], 
			            ALL('ffl_lineup'),
			            ALL('ffl week schedule'),
			            'ffl_lineup'[team] = Team1B,
			            'ffl week schedule'[Weeks ] = CurrentWeek
			        )
			        VAR Score2A = CALCULATE ( 
			            [FFL total points final v5], 
			            ALL('ffl_lineup'),
			            ALL('ffl week schedule'),
			            'ffl_lineup'[team] = Team2A,
			            'ffl week schedule'[Weeks ] = CurrentWeek
			        )
			        VAR Score2B = CALCULATE ( 
			            [FFL total points final v5], 
			            ALL('ffl_lineup'),
			            ALL('ffl week schedule'),
			            'ffl_lineup'[team] = Team2B,
			            'ffl week schedule'[Weeks ] = CurrentWeek
			        )
			
			        VAR Result =
			            SWITCH (
			                TRUE(),
			                Team1A = CurrentTeam && Score1A > Score1B, 1,
			                Team1B = CurrentTeam && Score1B > Score1A, 1,
			                Team2A = CurrentTeam && Score2A > Score2B, 1,
			                Team2B = CurrentTeam && Score2B > Score2A, 1,
			                0
			            )
			        RETURN Result
			    ),
			    BLANK()
			)
			```
